# Ethereum Configuration for Fusion+ Deployment
# Copy this file to .env and fill in your values

# Private key for deployment (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# Sepolia RPC URL (you can use Infura, Alchemy, or public RPC)
SEPOLIA_RPC_URL=https://rpc.sepolia.org

# Optional: Etherscan API key for contract verification
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# 1inch LOP Contract Address (will be updated with actual address)
INCH_LOP_CONTRACT=0x0000000000000000000000000000000000000001

# WETH Address on Sepolia
WETH_SEPOLIA=0xfFf9976782d46CC05630D1f6eBAb18b2324d6B14
